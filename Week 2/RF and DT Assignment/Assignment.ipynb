    {
      "cell_type": "code",
      "execution_count": 22,
      "metadata": {
        "id": "TwS9Ot2sYwJK"
      },
      "outputs": [],
      "source": [
        "from sklearn.ensemble import RandomForestClassifier\n",
        "\n",
        "clf_forest = RandomForestClassifier(n_estimators=10, criterion='gini', max_depth=10, random_state=42)\n",
        "\n",
        "clf_forest.fit(X_train, Y_train)\n",
        "rf_pred = clf_forest.predict(X_test)"
      ]
    },
